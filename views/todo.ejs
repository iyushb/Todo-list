<%- include ("./partials/header.ejs") %>

<div class="container">
    <h1>TO-DO LIST<i class="fa fa-angle-double-up"></i></h1>
    <form action="/todo" method="POST">
        <input type="text" name="text" placeholder="Add New TODO">
    </form>
    <ul>
        <%todo.forEach(function(items){%>
        <li>
            <span><i style=" font-size:24px" class=" fa fa-scissors "></i></span>
            <%=items.list%>
        </li>
        <%})%>
    </ul>
</div>

<%- include ("./partials/footer.ejs") %>